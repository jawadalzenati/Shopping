<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">@font/questv</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="ProgressDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/yellow</item>
        <item name="android:textColorPrimary">@color/gry</item>
    </style>


    <style name="SpinnerLook" parent="@android:style/Widget.TextView.SpinnerItem">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/gry</item>
    </style>


</resources>
